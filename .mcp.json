{
  "mcpServers": {
    "playwright": {
      "command": "cmd",
      "args": [
        "/c",
        "npx",
        "-y",
        "--registry",
        "https://registry.npmjs.org/",
        "@playwright/mcp",
        "--browser",
        "chrome",
        "--ignore-https-errors"
      ]
    }
  }
}
